<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="My Suite" verbose="2"  parallel="tests" thread-count="1">
    <listeners>
        <listener class-name="Listener.CucumberListener"/>
    </listeners>
    <test name="Tests suite" parallel="none">
        <classes>
            <class name="Runner.CucumberRunner" />
        </classes>
    </test>
</suite>